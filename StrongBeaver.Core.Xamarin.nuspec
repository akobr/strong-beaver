<?xml version="1.0"?>
<package>
	<metadata>
		<id>BeaverSoft.StrongBeaver.Xamarin</id>
		<version>0.9.0-alpha</version>
		<title>StrongBeaver Framework for Xamarin</title>
		<authors>BeaverSoft (Ales Kobr)</authors>
		<owners>BeaverSoft (Ales Kobr)</owners>

		<licenseUrl>https://github.com/akobr/strong-beaver/blob/master/LICENSE</licenseUrl>
		<projectUrl>https://github.com/akobr/strong-beaver</projectUrl>
		<iconUrl>https://github.com/akobr/strong-beaver/blob/master/doc/design/icon-nuget-package.png?raw=true</iconUrl>

		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>The mobile application Framework for Xamarin.</description>
		<releaseNotes>Alpha version of Xamarin multi-platform mobile application framework created by BeaverSoft.</releaseNotes>
		<copyright>Copyright 2017</copyright>
		<tags>Xamarin Mobile Framework iOS Android UWP Architecture MVVM PureMVC MVC Strong Beaver</tags>

		<dependencies>
			<group targetFramework="netstandard1.1">
				<dependency id="CommonServiceLocator" version="1.3.0" />
				<dependency id="MvvmLightLibs" version="5.3.0" />
				<dependency id="Newtonsoft.Json" version="10.0.3" />
				<dependency id="Xamarin.Forms" version="2.4.0.91020" />
				<dependency id="Rg.Plugins.Popup" version="1.0.4" />
				<dependency id="BeaverSoft.StrongBeaver.Core" version="0.9.0-alpha" />
			</group>
			<group targetFramework="MonoAndroid">
				<dependency id="CommonServiceLocator" version="1.3.0" />
				<dependency id="MvvmLightLibs" version="5.3.0" />
				<dependency id="Newtonsoft.Json" version="10.0.3" />
				<dependency id="Xamarin.Forms" version="2.4.0.91020" />
				<dependency id="Rg.Plugins.Popup" version="1.0.4" />
				<dependency id="BeaverSoft.StrongBeaver.Core" version="0.9.0-alpha" />
			</group>
			<group targetFramework="Xamarin.iOS">
				<dependency id="CommonServiceLocator" version="1.3.0" />
				<dependency id="MvvmLightLibs" version="5.3.0" />
				<dependency id="Newtonsoft.Json" version="10.0.3" />
				<dependency id="Xamarin.Forms" version="2.4.0.91020" />
				<dependency id="Rg.Plugins.Popup" version="1.0.4" />
				<dependency id="BeaverSoft.StrongBeaver.Core" version="0.9.0-alpha" />
			</group>
		</dependencies>
	</metadata>

	<files>
		<!-- Cross-platform reference assemblies (netstandard1.1) -->
		<file src="StrongBeaver.Core.Xamarin\bin\$configuration$\StrongBeaver.Core.Xamarin.dll" target="lib\netstandard1.1\StrongBeaver.Core.Xamarin.dll" />
		<file src="StrongBeaver.Core.Xamarin\bin\$configuration$\StrongBeaver.Core.Xamarin.pdb" target="lib\netstandard1.1\StrongBeaver.Core.Xamarin.pdb" />
		<file src="StrongBeaver.Core.Xamarin\bin\Debug\StrongBeaver.Core.Xamarin.xml" target="lib\netstandard1.1\StrongBeaver.Core.Xamarin.xml" />

		<!-- iOS reference assemblies -->
		<file src="StrongBeaver.Core.iOS\bin\$configuration$\StrongBeaver.Core.iOS.dll" target="lib\Xamarin.iOS\StrongBeaver.Core.iOS.dll" />
		<file src="StrongBeaver.Core.iOS\bin\$configuration$\StrongBeaver.Core.iOS.pdb" target="lib\Xamarin.iOS\StrongBeaver.Core.iOS.pdb" />
		<file src="StrongBeaver.Core.iOS\bin\Debug\StrongBeaver.Core.iOS.xml" target="lib\Xamarin.iOS\StrongBeaver.Core.iOS.xml" />

		<!-- Android reference assemblies -->
		<file src="StrongBeaver.Core.Droid\bin\$configuration$\StrongBeaver.Core.Droid.dll" target="lib\MonoAndroid\StrongBeaver.Core.Droid.dll" />
		<file src="StrongBeaver.Core.Droid\bin\$configuration$\StrongBeaver.Core.Droid.pdb" target="lib\MonoAndroid\StrongBeaver.Core.Droid.pdb" />
		<file src="StrongBeaver.Core.Droid\bin\Debug\StrongBeaver.Core.Droid.xml" target="lib\MonoAndroid\StrongBeaver.Core.Droid.xml" />
	</files>
</package>